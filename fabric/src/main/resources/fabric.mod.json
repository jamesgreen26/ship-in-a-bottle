{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "$mod_description",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${sources}"
  },
  "license": "${mod_license}",
  "icon": "assets/ship-in-a-bottle/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "g_mungus.ship_in_a_bottle.fabric.ShipInABottleModFabric"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "g_mungus.ship_in_a_bottle.fabric.ShipInABottleClientModFabric"
      }
    ]
  },
  "mixins": [
    "ship-in-a-bottle-common.mixins.json",
    "ship-in-a-bottle.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "valkyrienskies": ">=${vs2_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "architectury": ">=${architectury_version}"
  }
}